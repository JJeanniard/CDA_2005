<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tp_Freelance</title>
    <link rel="stylesheet" href="~/css/style.css" />
</head>
<body>
    <header>
        <div class="logo"><img src="~/img/TP_Freelancer_logo.jpg" /></div>
        <nav>
            <ul>
                <li><a href="/Customers/">Client</a></li>
                <li><a href="/Jobs/">Missions</a></li>
                <li><a href="/Quotes/">Devis & Factures</a></li>
            </ul>
        </nav>
    </header>
    <main role="main">
        @RenderBody()
    </main>
    <footer>
        <p> &copy; Copyright 2021 - Tp_Freelance - JEANNIARD Jonathan</p>
    </footer>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
